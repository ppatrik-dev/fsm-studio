{
    "automate_comment": "Timer to off, rozšířená verze na testovanie",
    "automate_name": "TOF5s",
    "inputs": [
        "var input = '1101001';"
    ],
    "outputs": [
        "var output;"
    ],
    "start_state": "A",
    "states": {
        "A": {
            "action": "out = '0';",
            "transitions": [
                {
                    "condition": "input[index] == '1'",
                    "to": "A"
                },
                {
                    "condition": "input[index] == '0'",
                    "to": "D"
                }
            ]
        },
        "B": {
            "action": "out = '1';",
            "transitions": [
                {
                    "condition": "input[index] == '1'",
                    "to": "C"
                }
            ]
        },
        "C": {
            "action": "",
            "transitions": [
                {
                    "condition": "input[index] == '0'",
                    "to": "A"
                },
                {
                    "condition": "@timeout",
                    "to": "B"
                }
            ]
        },
        "D": {
            "action": "x = x - 1;",
            "transitions": [
                {
                    "condition": "x <= 5",
                    "to": "E"
                },
                {
                    "condition": "input[index] == '1'",
                    "to": "A"
                }
            ]
        },
        "E": {
            "action": "out = '1';",
            "transitions": [
                {
                    "condition": "@timeout",
                    "to": "F"
                }
            ]
        },
        "F": {
            "action": "out = '0';",
            "transitions": [
                {
                    "condition": "true",
                    "to": "A"
                }
            ]
        }
    },
    "variables": [
        "var x = 10;"
    ]
}
