{
    "automate_comment": "Timer to off, jednoduch√° verze",
    "automate_name": "TOF5s",
    "inputs": [
        "var in = abcd;"
    ],
    "outputs": [
        "var out;"
    ],
    "start_state": "IDLE",
    "states": {
        "ACTIVE": {
            "action": "out = 1;",
            "transitions": [
                {
                    "condition": "in == 0",
                    "to": "TIMING"
                }
            ]
        },
        "IDLE": {
            "action": "out = 0;",
            "transitions": [
                {
                    "condition": "in == 1",
                    "to": "ACTIVE"
                }
            ]
        },
        "TIMING": {
            "action": "",
            "transitions": [
                {
                    "condition": "in == 1",
                    "to": "ACTIVE"
                },
                {
                    "condition": "@timeout",
                    "to": "IDLE"
                }
            ]
        }
    },
    "variables": [
        "var x = 10;"
    ]
}
