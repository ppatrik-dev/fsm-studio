{
    "automate_name": "TOF5s",
    "automate_comment": "Timer to off, jednoduch√° verze",
    "inputs": [
        "var in = abcd;"
    ],
    "outputs": [
        "var out;"
    ],
    "variables": [
        "var x = 10;"
    ],
    "start_state": "IDLE",
    "states": {
        "IDLE": {
            "action": "out = 0;",
            "transitions": [
                {
                    "to": "ACTIVE",
                    "condition": "in == 1"
                }
            ]
        },
        "ACTIVE": {
            "action": "out = 1;",
            "transitions": [
                {
                    "to": "TIMING",
                    "condition": "in == 0"
                }
            ]
        },
        "TIMING": {
            "action": "",
            "transitions": [
                {
                    "to": "ACTIVE",
                    "condition": "in == 1"
                },
                {
                    "to": "IDLE",
                    "condition": "@timeout"
                }
            ]
        }
    }
}