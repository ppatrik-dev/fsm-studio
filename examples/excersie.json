{
    "automate_comment": "",
    "automate_name": "",
    "inputs": [
        "var input = '1010';"
    ],
    "outputs": [
        "var output = '';"
    ],
    "start_state": "A",
    "states": {
        "A": {
            "action": "output = '0';",
            "transitions": [
                {
                    "condition": "input == '1';",
                    "to": "B"
                }
            ]
        },
        "B": {
            "action": "output = '1';",
            "transitions": [
                {
                    "condition": "input == '0';",
                    "to": "C"
                }
            ]
        },
        "C": {
            "action": "",
            "transitions": [
                {
                    "condition": "input == '1';",
                    "to": "B"
                },
                {
                    "condition": "moore.timer(timeout);",
                    "to": "A"
                }
            ]
        }
    },
    "variables": [
        "var timeout = 5000;"
    ]
}
