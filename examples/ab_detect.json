{
  "automate_comment": "Detekuje vzor 'ab*' vo vstupe",
  "automate_name": "PatternAbStar",
  "start_state": "S0",
  "inputs": [
    "var input = 'aabbbbab';"
  ],
  "outputs": [
    "var output = '';"
  ],
  "states": {
    "S0": {
      "action": "output += '-';",
      "transitions": [
        {
          "condition": "input == 'a';",
          "to": "S1"
        },
        {
          "condition": "input == 'b';",
          "to": "S0"
        }
      ]
    },
    "S1": {
      "action": "output += 'a';",
      "transitions": [
        {
          "condition": "input == 'b';",
          "to": "S1"
        },
        {
          "condition": "input == 'a';",
          "to": "S1"
        }
      ]
    }
  },
  "variables": []
}