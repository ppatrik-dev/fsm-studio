{
    "automate_comment": "Mooreov automat dekódujúci Morseho kód pre znaky E, A, T, M",
    "automate_name": "MorseEATM",
    "inputs": [
        "var input;"
    ],
    "outputs": [
        "var output = '';"
    ],
    "start_state": "ROOT",
    "states": {
        "ROOT": {
            "action": "output += '';",
            "transitions": [
                {
                    "condition": "input == '.';",
                    "to": "DOT"
                },
                {
                    "condition": "input == '-';",
                    "to": "DASH"
                }
            ]
        },
        "DOT": {
            "action": "output += '';",
            "transitions": [
                {
                    "condition": "input == '.';",
                    "to": "E"
                },
                {
                    "condition": "input == '-';",
                    "to": "A"
                }
            ]
        },
        "DASH": {
            "action": "output += '';",
            "transitions": [
                {
                    "condition": "input == '.';",
                    "to": "T"
                },
                {
                    "condition": "input == '-';",
                    "to": "M"
                }
            ]
        },
        "E": {
            "action": "output += 'E';",
            "transitions": [
                {
                    "condition": "input == ' ';",
                    "to": "ROOT"
                }
            ]
        },
        "A": {
            "action": "output += 'A';",
            "transitions": [
                {
                    "condition": "input == ' ';",
                    "to": "ROOT"
                }
            ]
        },
        "T": {
            "action": "output += 'T';",
            "transitions": [
                {
                    "condition": "input == ' ';",
                    "to": "ROOT"
                }
            ]
        },
        "M": {
            "action": "output += 'M';",
            "transitions": [
                {
                    "condition": "input == ' ';",
                    "to": "ROOT"
                }
            ]
        }
    },
    "variables": []
}