{
    "automate_comment": "A Moore machine that detects the occurrence of the sequence '101' and constructs the output string accordingly.",
    "automate_name": "Detect101WithTrace",
    "inputs": [
        "var input = '101101101';"
    ],
    "outputs": [
        "var output = '';",
        "var output_string = '';"
    ],
    "start_state": "S0",
    "states": {
        "S0": {
            "action": "output = '0'; output_string += output;",
            "transitions": [
                {
                    "condition": "input == '1';",
                    "to": "S1"
                },
                {
                    "condition": "input == '0';",
                    "to": "S0"
                }
            ]
        },
        "S1": {
            "action": "output = '0'; output_string += output;",
            "transitions": [
                {
                    "condition": "input == '0';",
                    "to": "S2"
                },
                {
                    "condition": "input == '1';",
                    "to": "S1"
                }
            ]
        },
        "S2": {
            "action": "output = '0'; output_string += output;",
            "transitions": [
                {
                    "condition": "input == '1';",
                    "to": "S3"
                },
                {
                    "condition": "input == '0';",
                    "to": "S0"
                }
            ]
        },
        "S3": {
            "action": "output = '1'; output_string += output;",
            "transitions": [
                {
                    "condition": "input == '1';",
                    "to": "S1"
                },
                {
                    "condition": "input == '0';",
                    "to": "S2"
                }
            ]
        }
    },
    "variables": [
    ]
}
