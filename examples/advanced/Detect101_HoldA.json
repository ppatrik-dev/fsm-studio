{
    "automate_comment": "A Moore machine that detects the sequence 101 through inputs inputA and inputB; inputA holds its last value after the sequence is completed",
    "automate_name": "Detect101_HoldA",
    "inputs": [
        "var inputA = '101101';",
        "var inputB = '101101';"
    ],
    "outputs": [
        "var output = '11';"
    ],
    "start_state": "S0",
    "states": {
        "S0": {
            "action": "",
            "transitions": [
                {
                    "condition": "inputA == '1' || inputB == '1';",
                    "to": "S1"
                },
                {
                    "condition": "true;",
                    "to": "S0"
                }
            ]
        },
        "S1": {
            "action": "",
            "transitions": [
                {
                    "condition": "inputA == '0' && inputB == '0';",
                    "to": "S2"
                },
                {
                    "condition": "true;",
                    "to": "S1"
                }
            ]
        },
        "S2": {
            "action": "",
            "transitions": [
                {
                    "condition": "inputA == '1' || inputB == '1';",
                    "to": "S3"
                },
                {
                    "condition": "true;",
                    "to": "S0"
                }
            ]
        },
        "S3": {
            "action": "output += '1';",
            "transitions": [
                {
                    "condition": "true;",
                    "to": "S1"
                }
            ]
        }
    },
    "variables": [
    ]
}
