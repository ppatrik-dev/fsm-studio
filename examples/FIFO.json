{
    "automate_comment": "Simuluje frontu veľkosti 2 s vložením a resetom",
    "automate_name": "FifoSimulator",
    "inputs": [
        "var char = '';"
    ],
    "outputs": [
        "var output = '';"
    ],
    "start_state": "Empty",
    "states": {
        "Empty": {
            "action": "output += 'E';",
            "transitions": [
                {
                    "condition": "char == 'i';",
                    "to": "Half"
                },
                {
                    "condition": "char == 'r';",
                    "to": "Empty"
                }
            ]
        },
        "Full": {
            "action": "output += 'F';",
            "transitions": [
                {
                    "condition": "char == 'i';",
                    "to": "Full"
                },
                {
                    "condition": "char == 'r';",
                    "to": "Empty"
                }
            ]
        },
        "Half": {
            "action": "output += 'H';",
            "transitions": [
                {
                    "condition": "char == 'i';",
                    "to": "Full"
                },
                {
                    "condition": "char == 'r';",
                    "to": "Empty"
                }
            ]
        }
    },
    "variables": [
    ]
}
